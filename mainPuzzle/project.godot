; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[android]

modules="org/godotengine/godot/GodotFAN"

[application]

config/name="puzzle"
run/main_scene="res://Scene/Lavels.tscn"
boot_splash/image="res://images/wallpaper/word 512.png"
boot_splash/fullsize=false
config/icon="res://images/logo/icon250.png"
config/auto_accept_quit=false
config/quit_on_go_back=false

[autoload]

StoreData="*res://Scene Code/store_data.gd"

[display]

window/size/width=600
window/size/height=1024
window/handheld/orientation="portrait"
window/stretch/mode="viewport"

[global]

touch=false

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/enable_pause_aware_picking=true
3d/active_soft_world=false
3d/godot_physics/use_bvh=false

[rendering]

environment/default_clear_color=Color( 0.0117647, 0.94902, 0.815686, 1 )
environment/default_environment="res://default_env.tres"
quality/dynamic_fonts/use_oversampling=false
